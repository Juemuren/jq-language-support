{
  "name": "jq-language-support",
  "displayName": "jq-language-support",
  "description": "jq Language Support for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "jq",
        "aliases": [
          "jq"
        ],
        "extensions": [
          ".jq"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jq",
        "scopeName": "source.jq",
        "path": "./syntaxes/jq.tmLanguage.json"
      }
    ]
  },
  "semanticTokenScopes": [
    {
      "language": "jq",
      "scopes": {
        "function": [
          "entity.name.function.jq"
        ],
        "parameter": [
          "variable.parameter.jq"
        ],
        "variable": [
          "variable.other.jq"
        ]
      }
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}